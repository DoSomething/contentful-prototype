{
  "private": true,
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "cypress": "cypress open",
    "cypress:ci": "cypress run -c baseUrl=http://127.0.0.1:8000 --record --reporter cypress-multi-reporters --reporter-options configFile=cypress-report-config.json --group functional",
    "lint": "eslint --ext .js resources/assets",
    "flow": "flow",
    "start": "npm run schema:fragments && webpack --env=development --hide-modules --watch",
    "build:dev": "npm run schema:fragments && webpack --env=development --hide-modules",
    "build": "npm run schema:fragments && webpack --env=production",
    "stat": "webpack --env=production --profile --json > stats.json",
    "analyze": "npm run stat && webpack-bundle-analyzer stats.json public/next/assets",
    "format": "prettier --write \"{docs,resources}/**/*.{js,md,scss,graphql}\"",
    "schema:fetch": "apollo service:download --endpoint https://graphql-dev.dosomething.org/graphql",
    "schema:fragments": "node bootstrap/getFragmentTypes.js",
    "heroku-postbuild": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "plugins": [
      "macros"
    ],
    "presets": [
      [
        "@dosomething",
        {
          "withPolyfills": false
        }
      ],
      "@babel/preset-flow",
      "@emotion/babel-preset-css-prop"
    ]
  },
  "browserslist": [
    ">0.5%",
    "ie 11",
    "not op_mini all"
  ],
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "clearMocks": true,
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/resources/assets/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@charlietango/use-script": "^2.2.0",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^13.4.0",
    "@dosomething/forge": "^6.10.0",
    "@dosomething/gateway": "^2.0.1",
    "@emotion/babel-preset-css-prop": "^10.2.1",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@react-pdf/renderer": "^1.6.12",
    "@typeform/embed": "^0.12.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-link-persisted-queries": "^0.2.2",
    "babel-jest": "^24.9.0",
    "classnames": "^2.2.6",
    "css-module-flow": "^1.0.0",
    "date-fns": "2.0.0-alpha.26",
    "dom-delegate": "^2.2.1",
    "dotenv": "^8.2.0",
    "environment-badge": "^1.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint-config-prettier": "^2.9.0",
    "fontfaceobserver": "^2.1.0",
    "graphql": "~14.6.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.11.0",
    "history": "^4.10.1",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "markdown-it": "^8.4.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-collapsible": "^1.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-for-inline": "^0.1.1",
    "node-fetch": "^2.6.1",
    "pluralize": "^8.0.0",
    "pretty-ms": "^7.0.1",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.14.0",
    "react-addons-update": "^15.6.3",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.14.0",
    "react-error-boundary": "^1.2.5",
    "react-intersection-observer": "^8.31.0",
    "react-loadable": "^5.4.0",
    "react-media": "^1.10.0",
    "react-portal": "^4.2.1",
    "react-redux": "^6.0.0",
    "react-required-if": "^1.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-prop-types": "^1.0.5",
    "react-select": "^3.2.0",
    "react-test-renderer": "^16.14.0",
    "react-tiny-popover": "^6.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sixpack-client": "^2.1.0",
    "tailwindcss": "^1.9.5",
    "twin.macro": "^1.12.1",
    "usa-states": "0.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-flow": "^7.12.1",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@dosomething/babel-preset": "^3.2.0",
    "@dosomething/eslint-config": "^5.0.1",
    "@dosomething/webpack-config": "^6.0.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/dom": "7.26.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.6.0",
    "apollo": "^2.32.1",
    "autoprefixer": "^9.8.6",
    "babel-plugin-macros": "^2.8.0",
    "bson": "^4.2.2",
    "contentful-management": "^7.5.1",
    "cypress": "^6.2.1",
    "cypress-file-upload": "^4.1.1",
    "cypress-graphql-mock": "github:DFurnes/cypress-graphql-mock#with-persisted-batch-support",
    "cypress-multi-reporters": "^1.4.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "faker": "^4.1.0",
    "flow-bin": "0.59.0",
    "graphql-tools": "^4.0.8",
    "husky": "^2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "minimist": "^1.2.0",
    "mocha": "^6.2.3",
    "mocha-junit-reporter": "^1.23.3",
    "modernizr": "^3.11.4",
    "postcss-loader": "^3.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "redux-devtools": "^3.7.0",
    "redux-devtools-dock-monitor": "^1.2.0",
    "redux-devtools-log-monitor": "^1.4.0",
    "turndown": "^5.0.3",
    "webpack": "~4.28.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "winston": "^3.3.3"
  }
}
