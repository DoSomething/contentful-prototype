{
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "~7.3.0",
    "ext-newrelic": "*",
    "barryvdh/laravel-debugbar": "^3.0",
    "contentful/contentful": "3.3.*",
    "contentful/laravel": "3.0.*",
    "dfurnes/environmentalist": "^0.0.2",
    "doctrine/dbal": "^2.5",
    "dosomething/gateway": "^3.0.0",
    "erusev/parsedown": "^1.6",
    "fideloper/proxy": "^4.0",
    "huddledigital/zendesk-laravel": "^3.2",
    "laravel/framework": "5.8.*",
    "predis/predis": "^1.1",
    "seatgeek/sixpack-php": "^2.1"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laravel/tinker": "~1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^7.0",
    "symfony/css-selector": "^4.0",
    "symfony/dom-crawler": "^4.0",
    "laravel/browser-kit-testing": "4.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/DatabaseSetup.php",
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "warmup": [
      "php artisan gateway:key",
      "php artisan config:cache"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
