{
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "barryvdh/laravel-debugbar": "^2.3",
    "contentful/contentful": "1.0.*",
    "contentful/laravel": "1.0.*",
    "dfurnes/environmentalist": "^0.0.2",
    "doctrine/dbal": "~2.5.0",
    "dosomething/gateway": "^1.14.7",
    "embed/embed": "3.2.*",
    "erusev/parsedown": "^1.6",
    "fideloper/proxy": "^3.3",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "predis/predis": "^1.1",
    "seatgeek/sixpack-php": "^2.1"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "laravel/browser-kit-testing": "2.*"
  },
  "autoload": {
    "classmap": ["database"],
    "files": ["app/helpers.php"],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/DatabaseSetup.php", "tests/TestCase.php"],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "warmup": ["php artisan gateway:key", "php artisan config:cache"]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
